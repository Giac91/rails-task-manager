<ul>
<% @tasks.each do |task| %>
  <li><h2><%= task.name %></h2>
    <p><%= link_to "See Description", "/tasks/#{task.id}"%></p>
    <p><%= link_to "Edit Task", "/tasks/#{task.id}/edit"%></p>
    <p><%= link_to "Delete", task_path(task),
      method: :delete,
      data: { confirm: "Are you sure?" } %></p>
  </li>
<% end %>
</ul>

<%= link_to "Create New...", "/tasks/new"%>
